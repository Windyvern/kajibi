{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Post"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "caption": {
      "type": "text"
    },
    "taken_at": {
      "type": "datetime"
    },
    "source": {
      "type": "string",
      "default": "instagram"
    },
    "source_id": {
      "type": "string",
      "unique": true
    },
    "media": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author"
    },
    "slug": {
      "type": "uid",
      "targetField": "caption"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "posts"
    },
    "longitude": {
      "type": "float"
    },
    "latitude": {
      "type": "float"
    },
    "address": {
      "type": "string"
    },
    "is_closed": {
      "type": "boolean",
      "default": false
    },
    "types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::type.type",
      "inversedBy": "posts"
    },
    "prizes": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::prize.prize",
      "inversedBy": "posts"
    },
    "lists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::list.list",
      "mappedBy": "posts"
    },
    "tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::tag.tag",
      "inversedBy": "posts"
    },
    "location": {
      "type": "customField",
      "customField": "plugin::google-maps.location-picker"
    }
  }
}
