{
  "kind": "collectionType",
  "collectionName": "lists",
  "info": {
    "singularName": "list",
    "pluralName": "lists",
    "displayName": "List"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "targetField": "name"
    },
    "description": {
      "type": "text",
      "required": false
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category"
    },
    "author": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::author.author"
    },
    "media": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "cover": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ]
    },
    "articles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::article.article",
      "mappedBy": "lists"
    },
    "list_type": {
      "type": "enumeration",
      "required": false,
      "default": "articles",
      "enum": [
        "articles",
        "media"
      ]
    },
    "disable_map_view": {
      "type": "boolean",
      "default": false
    },
    "reels": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::reel.reel",
      "inversedBy": "lists"
    },
    "posts": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::post.post",
      "inversedBy": "lists"
    },
    "latitude": {
      "type": "float",
      "required": false
    },
    "longitude": {
      "type": "float",
      "required": false
    },
    "location": {
      "type": "customField",
      "customField": "plugin::google-maps.location-picker"
    }
  }
}
